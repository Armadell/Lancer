{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb"
       
    ],
    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
       
        "arrow-body-style": ["off", "always"],
        
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-curly-newline": "off",
    "no-alert": "off",
    "prettier/prettier": "off",
    "semi": "off",
    "no-shadow": "off",
    
    "quotes": "off",
    "react/prop-types": "off",
    "react/jsx-no-useless-fragment": "off",
    "jsx-a11y/label-has-associated-control": "off"
        
        


    }
}
