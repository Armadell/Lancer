{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb"
       
    ],
    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier":"warn",
        "arrow-body-style": ["off", "always"],
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off"
        


    }
}
